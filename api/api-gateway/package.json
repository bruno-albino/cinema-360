{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "API Gateway provider for Cinema Project Services",
  "main": "./src/index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/"
  },
  "scripts": {
    "start": "npm run build && node build/index.js",
    "test": "jest",
    "start:dev": "nodemon -L ./src/index",
    "build": "rimraf ./build && tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.8",
    "@types/express-http-proxy": "^1.6.1",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.32",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "helmet": "^4.2.0",
    "http": "0.0.1-security",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "tap-spec": "^5.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
